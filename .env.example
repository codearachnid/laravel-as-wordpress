APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Start WordPress Constants
WP_TABLE_PREFIX=wp_
WP_DEBUG=true
WP_HOME=http://localhost
WP_SITEURL=http://localhost/wp
WP_CONTENT_DIR=/var/www/html/public/wp-content
WP_PLUGIN_DIR=/var/www/html/public/wp-content/plugins

# WordPress Salts (generate at https://api.wordpress.org/secret-key/1.1/salt/)
WP_AUTH_KEY=your-auth-key-here
WP_SECURE_AUTH_KEY=your-secure-auth-key-here
WP_LOGGED_IN_KEY=your-logged-in-key-here
WP_NONCE_KEY=your-nonce-key-here
WP_AUTH_SALT=your-auth-salt-here
WP_SECURE_AUTH_SALT=your-secure-auth-salt-here
WP_LOGGED_IN_SALT=your-logged-in-salt-here
WP_NONCE_SALT=your-nonce-salt-here

# WordPress Custom Settings
WP_DISABLE_UPDATES=false
WP_MAX_REVISIONS=5

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravelwordpress
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
